    <Grid>
        <Button x:Name="Cerrar" Content="Button" HorizontalAlignment="Left" Height="78" Margin="417,417,0,0" VerticalAlignment="Top" Width="80" Click="Cerrar_Click">
            <Button.BorderBrush>
                <ImageBrush/>
            </Button.BorderBrush>
            <Button.Foreground>
                <ImageBrush/>
            </Button.Foreground>
            <Button.OpacityMask>
                <ImageBrush/>
            </Button.OpacityMask>
            <Button.Background>
                <ImageBrush/>
            </Button.Background>
        </Button>

    </Grid>
</Window>
