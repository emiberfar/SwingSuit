        mc:Ignorable="d"
        Title="VentanaSobreNosotros" Height="505" Width="900" WindowStyle="None" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Icon="/SwingSuit;component/MenuPrincipal/diamond.png">
